  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "prettier"
    ]
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {}
    }
  },
  "packageManager": "pnpm@8.15.7"
}