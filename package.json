    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "framer-motion": "^11.9.0",
    "lucide-react": "^0.446.0",
    "next": "^14.2.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
